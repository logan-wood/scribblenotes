  <link href='https://fonts.googleapis.com/css?family=Poppins' rel='stylesheet'>

    <%- include('./partials/head.ejs'); %>



    <style>
        .accordion {
          background-color: rgb(255, 255, 255);
          color: #444;
          cursor: pointer;
          padding: 18px;
          width: 100%;
          border: none;
          text-align: left;
          outline: none;
          font-size: 15px;
          transition: 0.4s;
          text-align: center;
        }
        
        .active, .accordion:hover {
          background-color: #ccc; 
        }
        
        .panel {
          padding: 0 18px;
          display: none;
          background-color: white;
          overflow: hidden;
        }
        </style>


    <body style="font-family: Poppins;">

        <%- include('./partials/nav.ejs'); %>

        
<div class="form" id="dashboardContainer">
<div class="content">
  <h4 class="title">New Note </h4>
  <div class="card-base">
    <div class="card-base-wrapper">
      <h6>Download Template</h6>
      <div class="dashboard-button dashboard-success">
        Complete
      </div>

  <a style="text-align: center;" target="_blank" href="https://docs.google.com/spreadsheets/d/1hOC-ADPe_Aiv_cGbYfBlVTSlyOaQMZzc/edit#gid=724155891">Download Template Here!</a>
  
    </div>
  </div>
  <div class="card-base">
    <div class="dashboard-caption">
      <h6>Video Tutorial</h6>
      <div class="video">
