      </div>

    <div class="btn-wrapper">
      <input type="submit" value="SKIP" class="submit-btn" />
      <input type="submit" value="Pending.." class="pending-box" />
    </div>
    </div>
  </div>
  <div class="card-base">
    <div class="dashboard-caption">
      <h6>Upload Template</h6>

    <form name="submit" action="./new_note" method="post" enctype="multipart/form-data" id="uploadForm" class="upload-file">
      <label class="custom-file-upload">
      <input name="csv" type="file" />
      </label>
    </form>
    <div class="btn-wrapper">
      <input type="submit" value="Upload" class="submit-btn" />
    </div>
    </div>
  
  </div>
